{
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "七天酒店",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/hotel/hotel",
      "style": {
        "navigationBarTitleText": "酒店"
      }
    },
    {
      "path": "pages/service/service",
      "style": {
        "navigationBarTitleText": "服务"
      }
    },
    {
      "path": "pages/my/my",
      "style": {
        "navigationBarTitleText": "我的"
      }
    }
  ],
  "tabBar": {
    "color": "#999999",
    "selectedColor": "#C8A882",
    "backgroundColor": "#ffffff",
    "borderStyle": "black",
    "list": [
      {
        "pagePath": "pages/index/index",
        "text": "订房",
        "iconPath": "static/tabbar/home.png",
        "selectedIconPath": "static/tabbar/home-active.png"
      },
      {
        "pagePath": "pages/hotel/hotel",
        "text": "酒店",
        "iconPath": "static/tabbar/hotel.png",
        "selectedIconPath": "static/tabbar/hotel-active.png"
      },
      {
        "pagePath": "pages/service/service",
        "text": "服务",
        "iconPath": "static/tabbar/service.png",
        "selectedIconPath": "static/tabbar/service-active.png"
      },
      {
        "pagePath": "pages/my/my",
        "text": "我的",
        "iconPath": "static/tabbar/my.png",
        "selectedIconPath": "static/tabbar/my-active.png"
      }
    ]
  },
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "七天酒店",
    "navigationBarBackgroundColor": "#FFFFFF",
    "backgroundColor": "#F8F8F8"
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^tn-(.*)-(item|group)$": "@/uni_modules/tuniaoui-vue3/components/$1/src/$1-$2.vue",
      "^tn-(.*)": "@/uni_modules/tuniaoui-vue3/components/$1/src/$1.vue"
    }
